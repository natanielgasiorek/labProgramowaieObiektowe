<dx:ThemedWindow
    x:Class="Sklep_Zoologiczny.Widoki.Usuwanie_podkategorii"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
    Title="Usuwanie_podkategorii" Height="500" Width="500"
    >

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="371*"/>
            <ColumnDefinition Width="629*"/>
        </Grid.ColumnDefinitions>
        <Label Content="Usuń podkategorie:" HorizontalAlignment="Left" Margin="19,34,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
        <ComboBox x:Name="ComboBox_Podkategoria" HorizontalAlignment="Left" Margin="152,34,0,0" VerticalAlignment="Top" Width="238" Grid.ColumnSpan="2" SelectedItem="{Binding ID_Podkategorii}" DisplayMemberPath="Nazwa_podkategorii"/>
        <dx:SimpleButton x:Name="Zapisz_usuwanie_podkategorii" Content="Zapisz" HorizontalAlignment="Left" Height="41" Margin="182,103,0,0" VerticalAlignment="Top" Width="136" Glyph="{dx:DXImage SvgImages/XAF/Action_SaveScript.svg}" Click="Zapisz_usuwanie_podkategorii_Click" Grid.ColumnSpan="2"/>

    </Grid>
</dx:ThemedWindow>
