<dx:ThemedWindow
    x:Class="Sklep_Zoologiczny.Widoki.Dodawanie_kategorii"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
    Title="Dodawanie_kategorii" Height="500" Width="500"
    >

    <Grid>
        <Label Content="Nazwa kategorii:" HorizontalAlignment="Left" Margin="26,35,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
        <TextBox x:Name="Wpisz_nazwe_kategorii" HorizontalAlignment="Left" Margin="134,35,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="338"/>
        <dx:SimpleButton x:Name="Zapisz_nazwe_kategorii" Content="Zapisz" HorizontalAlignment="Center" Margin="0,101,0,328" Width="156" Glyph="{dx:DXImage SvgImages/XAF/Action_SaveScript.svg}" Click="Zapisz_nazwe_kategorii_Click"/>

    </Grid>
</dx:ThemedWindow>
