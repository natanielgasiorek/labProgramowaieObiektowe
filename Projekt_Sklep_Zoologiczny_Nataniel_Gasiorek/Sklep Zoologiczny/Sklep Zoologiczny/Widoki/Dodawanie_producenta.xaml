<dx:ThemedWindow
    x:Class="Sklep_Zoologiczny.Widoki.Dodawanie_producenta"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
    Title="Dodawanie_producenta" Height="500" Width="500"
    >

    <Grid>
        <Label Content="Nazwa producenta:" HorizontalAlignment="Left" Margin="22,31,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
        <TextBox x:Name="Wpisz_nazwe_producenta" HorizontalAlignment="Left" Margin="168,31,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="276"/>
        <dx:SimpleButton x:Name="Zapisz_nazwe_producenta" Content="Zapisz" HorizontalAlignment="Center" Height="39" Margin="0,103,0,0" VerticalAlignment="Top" Width="112" Glyph="{dx:DXImage SvgImages/XAF/Action_SaveScript.svg}" Click="Zapisz_nazwe_producenta_Click"/>

    </Grid>
</dx:ThemedWindow>
